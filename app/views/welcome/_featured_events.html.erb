<div class="card-deck">
  

  <% events.each do |event| %>

    <div class="card">
      <div class="card-block">
      <h4 class="card-title"> 
        <%= link_to event.title, event%>
      </h4>

      <p class="card-text">
        <small class="text-muted"> Updated <%= time_ago_in_words(event.updated_at) %> ago
        </small>
      </p>
      </div>
    </div>

  <% end %>


</div>